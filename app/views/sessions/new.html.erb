
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
    
    <%= f.label :email %>
    <%= f.email_field :email, class: "field" %>
    <%= f.label :password %>
    <%= f.password_field :password, class: "field" %></br>
    <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>
